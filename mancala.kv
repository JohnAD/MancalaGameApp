#:kivy 1.9.1
#:import NoTransition kivy.uix.screenmanager.NoTransition
#:import settings __main__.settings
#:import character __main__.character
#:import update_setting __main__.update_setting
#:import visual_settings __main__.visual_settings

AppScreenManager:
    transition: NoTransition()
    GameScreen:
    SettingsOpponentScreen:
    SettingsRulesScreen:
    SettingsScreenScreen:
    SettingsSoundScreen:

<SettingsOpponentScreen>
    name: "settings_opponent_screen"
    FloatLayout:
        Image:
            id: board_image
            source: 'assets/img/settings-background.png'
            pos_hint: {'x': -0.5*(2400-1920)/1920.0, 'y': -0.5*(2400-1080)/1080.0}
            size_hint: (2400/1920.0, 2400/1080.0)
        FixedLayout:
            Button:
                background_normal: 'assets/img/not-rules.png'
                pos_fixed: (-190, 478)
                size_fixed: (498, 218)
                on_release: app.root.current = "settings_rules_screen"
            Button:
                background_normal: 'assets/img/not-screen.png'
                pos_fixed: (-190, 275)
                size_fixed: (498, 205)
                on_release: app.root.current = "settings_screen_screen"
            Button:
                background_normal: 'assets/img/not-sound.png'
                pos_fixed: (-191, -14)
                size_fixed: (499, 289)
                on_release: app.root.current = "settings_sound_screen"
            Button:
                id: resume_game
                pos_fixed: (1300, 0)
                size_fixed: (250, 100)
                background_normal: 'assets/img/blank-wood-button.png'
                font_size_fixed: 56
                text: "Resume"
                color: (101/255.0, 244/255.0, 66/255.0, 1)
                on_release: app.resume_game()
            Button:
                pos_fixed: (1600, 0)
                size_fixed: (300, 100)
                text: "New Game"
                on_release: app.start_new_game()
                font_size_fixed: 50
                color: (0, 0, 0, 1)
                background_color: (0.1, 1, 0.1, 1)
            Image:
                id: ai_face_image
                pos_fixed: (1920-410+55, 1080-417+58)
                size_fixed: (300, 300)
                source: 'assets/img/blue3.png'
            Image:
                pos_fixed: (1920-410, 1080-417)
                size_fixed: (410, 417)
                source: 'assets/img/picture-frame.png'
            Button:
                text: "Learn How to Play\n(YouTube)"
                pos_fixed: (1920 - 410, 1080 - 417 - 300)
                size_fixed: (410, 150)
                color: (0, 0, 0, 1)
                background_normal: 'assets/img/youtube-url.png'
                font_size_fixed: 56
                on_release: root.launch_url()
            Label:
                id: ai_rank
                pos_fixed: (1920 - 410/2, 1080 - 417 - 100)
                size_fixed: (200, 100)
                text_size: self.size
                font_size_fixed: 56
                color: (0, 0, 0, 1)
                markup: True
                text: 'n of 12'
            Label:
                id: ai_name
                pos_fixed: (400, 800)
                size_fixed: (1200, 200)
                text_size: self.size
                font_size_fixed: 96
                color: (0, 0, 0, 1)
                markup: True
                text: character['name']
            Label:
                id: ai_description
                pos_fixed: (400, 400)
                size_fixed: (1100, 350)
                text_size: self.size
                font_size_fixed: 64
                color: (0, 0, 0, 1)
                valign: "top"
                markup: True
                text: character['desc']
            Label:
                id: ai_play_style
                pos_fixed: (400, 300)
                size_fixed: (1100, 250)
                text_size: self.size
                font_size_fixed: 64
                color: (0, 0, 0, 1)
                markup: True
                valign: "top"
                text: "[i]"+character['tagline']+"[/i]"
            Button:
                id: prev_ai
                pos_fixed: (400, 0)
                size_fixed: (200, 200)
                background_normal: 'assets/img/prev-button.png'
                on_release: root.previous_ai()
            Label:
                id: choose_msg
                pos_fixed: (960, 250)
                size_fixed: (100, 100)
                color: (0, 0, 0.2, 1)
                font_size_fixed: 56
                markup: True
                text: ""
            Button:
                id: choose_ai
                pos_fixed: (700, 50)
                size_fixed: (250, 100)
                background_normal: 'assets/img/blank-wood-button.png'
                font_size_fixed: 56
                text: ""
                color: (101/255.0, 244/255.0, 66/255.0, 1)
                on_release: root.choose_ai()
            Button:
                id: next_ai
                pos_fixed: (1050, 0)
                size_fixed: (200, 200)
                background_normal: 'assets/img/next-button.png'
                on_release: root.next_ai()

